@{
    ViewBag.Calc
}

@model IQueryable<Books>

<h2>Bookstore list of books</h2>

@foreach(Books b in Model)
            {
    <h3>@b.Title</h3>
    <ul>
        <li>Author: @b.Author</li>
        <li>Publisher: @b.Publisher</li>
        <li>ISBN Number: @b.Isbn</li>
        <li>Category: @b.Category</li>
        <li>Classification: @b.Classification</li>
        <li>Page count: @b.PageCount</li>
        <li>Book price: @b.Price</li>
    </ul>

    <br />

            }

<a asp-controller="Home" asp-action="Index" asp-route-pageNum ="1">1</a>
<a asp-controller="Home" asp-action="Index" asp-route-pageNum ="2">2</a>
<a asp-controller="Home" asp-action="Index" asp-route-pageNum ="3">3</a>

<div></div>